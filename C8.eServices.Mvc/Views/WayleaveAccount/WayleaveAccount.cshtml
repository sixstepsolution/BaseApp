<script>
    $(function () {
        $("#tradeLicenseExpirationDate").datepicker({
            changeMonth: true,
            changeYear: true,
            dateFormat: 'dd-mm-yy',
            onSelect: function (dateString, txtDate) {
                if (dateString != undefined && dateString != "" && dateString != null) {
                    var dt = dateString;
                    var ar = dt.split('-');
                    var test = ar[2] + "-" + ar[1] + "-" + ar[0];
                    $("#hdntradeLicenseExpirationDate").val(test);
                }
                else {
                    toastr.warning('Please select trade license expiration date!', "warning", {
                        "timeOut": "30000",
                        "extendedTImeout": "50000",
                        "closeButton": true,
                    });
                }
            }
        });
    });
</script>
