</style>
<script src="~/Scripts/wayleaveScripts/WayleaveAccountDashboard.js"></script>
<link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
@*<script src="https://code.jquery.com/jquery-1.12.4.js"></script>*@
@*<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>*@
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
<style>
    .ui-autocomplete {
        max-height: 200px;
        max-width: 170px !important;
        overflow-y: auto;
        /* prevent horizontal scrollbar */
        overflow-x: auto;
        /* add padding to account for vertical scrollbar */
        padding: 5px !important;
        font-size: 12pt !important;
        border: 1px solid #00a65a !important;
        color: #000 !important;
    }
</style>
<div>
    <section class="content" style="padding-right: 0;padding-top:0;">
        @using (Html.BeginForm("WayleaveAccountDashboard", "WL", FormMethod.Post, new { @class = "form-horizontal", role = "form", name = "FilterForm" }))
        {
            <div class="panel-group" style="margin-left: -25px;margin-bottom: 0px;margin-right: -14px;">
                <div class="panel panel-default" style="border: none;">
                    <div class="panel-heading" style="background:#d2930b">
                        <h4 class="panel-title">
                            <a data-toggle="collapse" data-parent="#accordionSerach" href="#collapse_search_1" style="color:#fff!important">
                                <i class="fa fa-plus" style="display: none;font-size: 12px;top: 0;"></i>
                                <i class="fa fa-minus" style="font-size: 12px;top: 0;"></i>
                                Search
                            </a>
                        </h4>
                    </div>
                    <div id="collapse_search_1" class="panel-collapse collapse in" style="border: 2px solid #b59b5a;">
                        <div class="panel-body" style="padding: 0;">

                            <table>
                                <tr>
                                    <td style="width:  15.5%">
                                        <label>Account Number</label>
                                        <div class="input-group" style="width: 100%;">
                                            @Html.TextBoxFor(m => m.account_number, new { @class = "form-control bradius", autocomplete = "off", name = "account_number", placeholder = "Account Number" })
                                            <span class="input-group-addon"
                                                  style="border-top-left-radius: 0;border-bottom-left-radius: 0;padding-bottom: 0;padding-top: 0px;">
                                                <i class="fa fa-sort-numeric-asc"></i>
                                            </span>
                                        </div>
                                    </td>
                                    <td style="width:  15.5%">
                                        <label>Person name</label>
                                        <div class="input-group" style="width: 100%;">
                                            @Html.TextBoxFor(m => m.contactPersonName, new { @class = "form-control bradius", autocomplete = "off", name = "contactPersonName", placeholder = "Person Name" })
                                            <span class="input-group-addon"
                                                  style="border-top-left-radius: 0;border-bottom-left-radius: 0;padding-bottom: 0;padding-top: 0px;">
                                                <i class="fa fa-user"></i>
                                            </span>
                                        </div>
                                    </td>
                                    <td style="width:  15.5%">
                                        <label>Person Surname</label>
                                        <div class="input-group" style="width: 100%;">
                                            @Html.TextBoxFor(m => m.contactPersonLastName, new { @class = "form-control bradius", autocomplete = "off", name = "contactPersonLastName", placeholder = "Person Last Name" })
                                            <span class="input-group-addon"
                                                  style="border-top-left-radius: 0;border-bottom-left-radius: 0;padding-bottom: 0;padding-top: 0px;">
                                                <i class="fa fa-user"></i>
                                            </span>
                                        </div>
                                    </td>
                                    <td style="width:  15.5%">
                                        <label>User Type</label>
                                        <div class="input-group input-label" style="width: 100%;margin-top:0px!important">

                                            @Html.DropDownList("userType", (IEnumerable
        <SelectListItem>)ViewBag.userTypeList, "Make a selection",
                    new { @class = "form-control", name = "userType" })
                                        </div>
                                    </td>
                                    <td style="width:  15.5%">
                                        <label>Company Name</label>
                                        <div class="input-group" style="width: 100%;">
                                            @Html.TextBoxFor(m => m.companyFullName, new { @class = "form-control bradius", autocomplete = "off", name = "companyFullName", placeholder = "Company Name" })
                                            <span class="input-group-addon" style="border-top-left-radius: 0;border-bottom-left-radius: 0;padding-bottom: 0;padding-top: 0px;">
                                                <i class="fa fa-building"></i>
                                            </span>
                                        </div>
                                    </td>
                                    <td style="width:  15.5%">
                                        <div class="input-group input-label" style="width: 100%;margin-top:0px!important">
                                            <label>Status Type</label>
                                            <select style="border-radius: 4px" id="AppStatusType"
                                                    class="form-control" name="AppStatusType">
                                                <option value="" selected>Make a selection</option>
                                            </select>
                                        </div>
                                    </td>
                                </tr>
                                <tr>
                                    <td style="width:  15.5%">
                                        <label>Email</label>
                                        <div class="input-group" style="width: 100%;margin-top:0px!important">
                                            @Html.TextBoxFor(m => m.email, new { @class = "form-control bradius", autocomplete = "off", name = "email", placeholder = "Email" })
                                            <span class="input-group-addon"
                                                  style="border-top-left-radius: 0;border-bottom-left-radius: 0;padding-bottom: 0;padding-top: 0px;">
                                                <i class="fa fa-envelope"></i>
                                            </span>
                                        </div>
                                    </td>
                                    <td style="width:  15.5%">
                                        <label>Mobile</label>
                                        <div class="input-group" style="width: 100%;">
                                            @Html.TextBoxFor(m => m.mobileNumber, new { @class = "form-control bradius", autocomplete = "off", name = "mobileNumber", placeholder = "Mobile Number" })
                                            <span class="input-group-addon"
                                                  style="border-top-left-radius: 0;border-bottom-left-radius: 0;padding-bottom: 0;padding-top: 0px;">
                                                <i class="fa fa-mobile"></i>
                                            </span>
                                        </div>
                                    </td>
                                    <td style="width:  15.5%">
                                        <label>Date Requested From</label>
                                        <div class="input-group" style="width: 100%;margin-top:0px!important">
                                            <input class="form-control datepicker" type="text" style=" border-top-right-radius: 0px !important; border-bottom-right-radius: 0px !important" name="date_requested_from" placeholder="Date Requested From" id="date_requested_from">

                                            <span class="input-group-addon"
                                                  style="border-top-left-radius: 0;border-bottom-left-radius: 0;padding-bottom: 0;padding-top: 0px;">
                                                <i class="fa fa-calendar"></i>
                                            </span>
                                        </div>
                                    </td>
                                    <td style="width:  15.5%">
                                        <label>Date Requested To</label>
                                        <div class="input-group" style="width: 100%;margin-top:0px!important">
                                            <input class="form-control datepicker" name="date_requested_from" placeholder="Date Requested To" id="date_requested_to" style=" border-top-right-radius: 0px !important; border-bottom-right-radius: 0px !important">
                                            <span class="input-group-addon"
                                                  style="border-top-left-radius: 0;border-bottom-left-radius: 0;padding-bottom: 0;padding-top: 0px;">
                                                <i class="fa fa-calendar"></i>
                                            </span>
                                        </div>
                                    </td>
                                    <td style="width:  15.5%">
                                        <label>Created Date</label>
                                        <div class="input-group" style="width: 100%;margin-top:0px!important">
                                            <input class="form-control datepicker" name="created_date" placeholder="Created date" id="created_date" style=" border-top-right-radius: 0px !important; border-bottom-right-radius: 0px !important">
                                            <span class="input-group-addon"
                                                  style="border-top-left-radius: 0;border-bottom-left-radius: 0;padding-bottom: 0;padding-top: 0px;">
                                                <i class="fa fa-calendar"></i>
                                            </span>
                                        </div>
                                    </td>
                                    <td style="width:  15.5%;text-align: center;">
                                        <div class="col-md-3" style="margin-top:18px">
                                            <button type="submit" onclick="$.fn.filterWayleaveByKeywords()"
                                                    class="btn btn-green-custom" style="height: 24px; padding: 2px; font-size: 12px; width: 100px;color:#fff!important">
                                                <i class="fa fa-spinner fa-spin" style="font-size:9pt;display:none!important" id="spinnerLoader"></i>
                                                Filter
                                            </button>
                                        </div>
                                        <div class="col-md-3" style="margin-top:18px;margin-left:60px;margin-right:45px">
                                            <button type="reset" ng-click=""
                                                    class="btn btn-success" style="height: 24px; padding: 2px; font-size: 12px; width: 100px;"
                                                    tabindex="20">
                                                Clear
                                            </button>
                                        </div>
                                    </td>
                                </tr>
                                <tr>
                                </tr>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <div style="background-color:white;margin-top:4px">
                <div id="myTabContent" class="tab-content">
                    <div class="row">
                        <div class="col-lg-12">
                            <div style="margin-bottom: 0;margin-right: -14px;">
                                <table class="table table-striped table-bordered table-hover tabel-condensed"
                                       style="font-size: 11px;">
                                    <thead style="background-color:#d2930b !important; font-size:11px !important;color:#fff !important;margin-bottom: 0;">
                                        <tr>
                                            <td style="width:5% !important" ng-click="sort('NAME')"
                                                ng-class="sortClass('NAME')">
                                                <a class="anchor_sorting" href="">
                                                    Id
                                                </a>&nbsp;
                                            </td>
                                            <td style="width:10% !important" ng-click="sort('NAME')"
                                                ng-class="sortClass('NAME')">
                                                <a class="anchor_sorting" href="">
                                                    Account Number
                                                </a>&nbsp;
                                            </td>
                                            <td style="width:7% !important" ng-click="sort('NAME')"
                                                ng-class="sortClass('NAME')">
                                                <a class="anchor_sorting" href="">
                                                    User Type
                                                </a>&nbsp;
                                            </td>
                                            <td style="width:10% !important" ng-click="sort('NAME')"
                                                ng-class="sortClass('NAME')">
                                                <a class="anchor_sorting" href="">
                                                    Company Name
                                                </a>&nbsp;
                                            </td>
                                            <td style="width:10% !important" ng-click="sort('NAME')"
                                                ng-class="sortClass('NAME')">
                                                <a class="anchor_sorting" href="">
                                                    Person Name
                                                </a>&nbsp;
                                            </td>
                                            <td style="width:18% !important" ng-click="sort('CREATED_DATE')"
                                                ng-class="sortClass('CREATED_DATE')">
                                                <a class="anchor_sorting" href="">
                                                    Email
                                                </a>&nbsp;
                                            </td>
                                            <td style="width:20% !important" ng-click="sort('CREATED_DATE')"
                                                ng-class="sortClass('CREATED_DATE')">
                                                <a class="anchor_sorting" href="">
                                                    Address
                                                </a>&nbsp;
                                            </td>

                                            <td style="width:10% !important" ng-click="sort('STATUS')"
                                                ng-class="sortClass('STATUS')">
                                                <a class="anchor_sorting" href="">
                                                    Status
                                                </a>&nbsp;
                                            </td>
                                            <td style="width:10% !important" ng-click="sort('CREATED_DATE')"
                                                ng-class="sortClass('CREATED_DATE')">
                                                <a class="anchor_sorting" href="">
                                                    Created On
                                                </a>&nbsp;
                                            </td>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td colspan="10" style="padding:0">
                                                <div class="panel-group">
                                                    <div class="bs-example">
                                                        <div class="panel-group" id="accordionpatient"
                                                             style="background-color: white;">
                                                            @*<i ng-if="accountCounts.length==0" class="fa fa-spinner fa-spin" style="font-size:9pt;color:#cc0c00"></i>*@
                                                            <div class="panel panel-default-custom">
                                                                @if (ViewBag.WayleaveAccountDataCount > 0)
                                                                {
                                                                    foreach (var rc in (List<C8.eServices.Mvc.Models.Mapings.WayleaveAccountDashboardModel>)ViewBag.WayleaveAccountData)
                                                                    {
                                                                        <div class="panel-heading" style="padding: 4px 14px;" id="panelSection_@rc.id">
                                                                            <h4 class="panel-title">
                                                                                <a data-toggle="collapse"
                                                                                   @*onclick="addIconCollapse('@rc.name','@rc.id')"*@
                                                                                   data-parent="#accordionpatient"
                                                                                   href="#collapse_@rc.id" class="collapsed"
                                                                                   aria-expanded="false"
                                                                                   onMouseOver="this.style.color='#000'"
                                                                                   onMouseOut="this.style.color='#000'">
                                                                                    <span class="glyphicon glyphicon-triangle-bottom span-overide"
                                                                                          style="left: -40px;color: #00A65A;"></span><span style="margin-left: -26px;font-size: 14px;padding: 2px;">
                                                                                        @rc.name (@rc.count)
                                                                                    </span>
                                                                                    @if (rc.id == 0)
                                                                                    {
                                                                                        <i class="fa fa-spinner fa-spin" style="font-size:9pt;color:#cc0c00"></i>
                                                                                    }

                                                                                </a>
                                                                            </h4>
                                                                        </div>
                                                                        <div id="collapse_@rc.id"
                                                                             class="panel-collapse collapse"
                                                                             aria-expanded="false"
                                                                             onMouseOver="this.style.color='#000'"
                                                                             onMouseOut="this.style.color='#000'"
                                                                             style="height: 0px;">
                                                                            <div class="panel-body"
                                                                                 style="overflow-y:scroll ;padding: 0;min-height: 20px !important;max-height: 300px !important;">
                                                                                <table class="table table-striped table-hover tabel-condensed">
                                                                                    <tbody>
                                                                                        @foreach (var app in rc.accountList)
                                                                                        {
                                                                                            <tr style="border-collapse:collapse;">
                                                                                                <td style="width:5% !important">
                                                                                                    <a onclick="goToWayleaveAccount('@tt.Encrypt(app.account_number.ToString())')"
                                                                                                       style="cursor: pointer;"
                                                                                                       class="ggb-a"><u> @app.account_id</u></a>
                                                                                                </td>
                                                                                                <td style="width:11% !important">
                                                                                                    @app.account_number
                                                                                                </td>
                                                                                                <td style="width:7% !important">
                                                                                                    @app.userType
                                                                                                </td>
                                                                                                <td style="width:10% !important">
                                                                                                    @app.companyFullName
                                                                                                </td>
                                                                                                <td style="width:10% !important">
                                                                                                    @app.contactPersonName
                                                                                                </td>
                                                                                                <td style="width:18% !important">
                                                                                                    @app.email
                                                                                                </td>
                                                                                                <td style="width:23% !important">
                                                                                                    @app.address
                                                                                                </td>
                                                                                                <td style="width:10% !important">
                                                                                                    @if (app.status == "Pending")
                                                                                                    {
                                                                                                        <div style="color:red">
                                                                                                            @app.status
                                                                                                        </div>
                                                                                                    }
                                                                                                    @if (app.status == "Approved")
                                                                                                    {
                                                                                                        <div style="color:green">
                                                                                                            @app.status
                                                                                                        </div>
                                                                                                    }
                                                                                                    @if (app.status == "Rejected")
                                                                                                    {
                                                                                                        <div style="color:red">
                                                                                                            @app.status
                                                                                                        </div>
                                                                                                    }
                                                                                                </td>
                                                                                                <td style="width:10% !important">
                                                                                                    @app.createdDate
                                                                                                </td>
                                                                                            </tr>
                                                                                        }
                                                                                </table>
                                                                            </div>
                                                                        </div>
                                                                    }
                                                                }
                                                                else
                                                                {
                                                                    <div class="text-center"><b class="text-red">No result found!</b></div>
                                                                }
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="tab-pane active in" id="AuditLog">
                </div>
                <div class="tab-pane" id="Documents">
                </div>
                <div class="tab-pane" id="Communication">
                </div>
                <div class="tab-pane" id="CivilLawsuits">
                </div>
                <div class="tab-pane" id="Finding">
                </div>
                <div class="tab-pane" id="Recommendation">
                </div>
                <div class="tab-pane" id="Case">
                </div>
            </div>
        }
    </section>
</div>
