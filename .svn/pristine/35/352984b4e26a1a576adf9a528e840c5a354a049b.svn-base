@{
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}
<head>

    <link href="~/Content/DashboardStyle.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/font-awesome.min.css">
    <script src="~/Scripts/f4f8b79905.js"></script>
</head>

<style>
    @@media only screen and (min-device-width: 850px) and (-webkit-min-device-pixel-ratio: 1) {

        .col-lg-3 {
            width: 20%;
        }
}
   



    .mb-4, .my-4 {
        margin-bottom: 1.5rem !important;
    }

    .bg-primary {
        background-color: #d2930b !important;
    }

    .rounded {
        border-radius: .25rem !important
    }

    h5 {
        color: white;
    }

    .row-spaces {
        margin-top: 30px;
    }

    .infoPanel {
        height: 350px;
    }

    .tbcheader {
        color: white !important;
    }

    @@media screen and (-ms-high-contrast: active), (-ms-high-contrast: none) {
        .flip-container:hover .flip {
            -webkit-transform: rotateY(10deg);
            transform: rotateY(10deg);
        }
    }

    @@media screen and (min-color-index:0) and(-webkit-min-device-pixel-ratio:0) {
        @@media {
            .flip-container:hover .flip {
                -webkit-transform: rotateY(10deg);
                transform: rotateY(10deg);
            }
        }
    }
</style>
