<body>
    @{Html.RenderAction("RequestNotifications", "Requests");}
    <div class="container-fluid" style="margin-top:-3%">
        <div class="row">
            <a href="~/Profile/Index">
                <div class="col-sm-12 col-md-6 col-lg-3 mb-4">
                    <div class="flip-container">
                        <div class="flip">
                            <div class="card bg-primary text-white text-center rounded card-dashbody d-flex flex-column justify-content-center">
                                <div class="card-dashbody d-flex flex-column justify-content-center">
                                    <p>
                                        <br />
                                        <i class="fas fa-id-card-alt fa-3x" style="margin-top:9%"></i>
                                    </p>
                                    <h5 class="card-title">My Profile</h5>
                                    <p class="card-text"></p>
                                </div>
                            </div>
                            <div class="card flip-back">
                                <div class="card bg-primary text-white text-center rounded card-dashbody d-flex flex-column justify-content-center">
                                    <p>
                                        <br /><i class="fas fa-id-card-alt fa-3x"></i>
                                    </p>


                                    <h5 class="card-title">My Profile</h5>
                                    <p class="card-text">View Your Profile Information and Link Municipal Accounts</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </a>
            <a href="~/Profile/AccountRequests">
                <div class="col-sm-12 col-md-6 col-lg-3 mb-4">
                    <div class="flip-container">
                        <div class="flip">
                            <div class="card bg-primary text-white text-center rounded card-dashbody d-flex flex-column justify-content-center">
                                <div class="card-dashbody d-flex flex-column justify-content-center">
                                    <p>
                                        <br />
                                        <i class="fas fa-id-card-alt fa-3x" style="margin-top:9%"></i>
                                    </p>
                                    <h5 class="card-title">Account Access Requests</h5>
                                    <p class="card-text"></p>
                                    <br />
                                    <span style="margin-top: -20px;padding: 10px 10px 10px 10px !important;" class="badge alert-warning" id="fullbatchCount1">@Session["ReqCount"]</span>
                                </div>
                            </div>
                            <div class="card flip-back">
                                <div class="card bg-primary text-white text-center rounded card-dashbody d-flex flex-column justify-content-center">
                                    <p>
                                        <br /><i class="fas fa-id-card-alt fa-3x"></i>
                                    </p>


                                    <h5 class="card-title">Account Access Requests</h5>
                                    <p class="card-text">Send a request to access a municipal account that has already been linked to another Siyakhokha user.</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </a>
            <a href="~/BankAccount/Index">
                <div class="col-sm-12 col-md-6 col-lg-3 mb-4">
                    <div class="flip-container">
                        <div class="flip">
                            <div class="card bg-primary text-white rounded text-center flip-front">
                                <div class="card-dashbody d-flex flex-column justify-content-center">
                                    <p>
                                        <br /><i class="fas fa-money-check fa-3x" style="margin-top:9%"></i>

                                    </p>
                                    <h5 class="card-title">My Bank Accounts</h5>
                                    <p class="card-text"></p>
                                </div>
                            </div>
                            <div class="card flip-back">
                                <div class="card bg-primary text-white text-center rounded card-dashbody d-flex flex-column justify-content-center">
                                    <p>
                                        <br /><i class="fas fa-money-check fa-3x"></i>
                                    </p>
                                    <h5 class="card-title">My Bank Accounts</h5>
                                    <p class="card-text">View Your Current Bank Accounts and Add New Bank Accounts </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </a>
            <a href="~/InstantEFT/IndexBatchEFTPayment">
                <div class="col-sm-12 col-md-6 col-lg-3 mb-4">
                    <div class="flip-container">
                        <div class="flip">
                            <div class="card bg-primary text-white rounded text-center flip-front">
                                <div class="card-dashbody d-flex flex-column justify-content-center">
                                    <p>
                                        <br /><i class="fas fa-money-check-alt fa-3x" style="margin-top:9%"></i>
                                    </p>
                                    <h5 class="card-title">Instant EFT Payments</h5>
                                    <p class="card-text"></p>
                                </div>
                            </div>
                            <div class="card flip-back">
                                <div class="card bg-primary text-white text-center rounded card-dashbody d-flex flex-column justify-content-center">
                                    <p>
                                        <br /><i class="fas fa-money-check-alt fa-3x" style="margin-top:9%"></i>
                                    </p>
                                    <h5 class="card-title">Instant EFT Payments</h5>
                                    <p class="card-text">Make a Single or Batch(multiple municipal accounts) Instant EFT Payment</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </a>
            <a href="~/DebitOrder/Index">
                <div class="col-sm-12 col-md-6 col-lg-3 mb-4">
                    <div class="flip-container">
                        <div class="flip">
                            <div class="card bg-primary text-white rounded text-center flip-front">
                                <div class="card-dashbody d-flex flex-column justify-content-center">
                                    <p>
                                        <br /><i class="fas fa-money-check-alt fa-3x" style="margin-top:9%"></i>
                                    </p>
                                    <h5 class="card-title">Once-Off Payments/ Debit Orders</h5>
                                    <p class="card-text"></p>
                                </div>
                            </div>
                            <div class="card flip-back">
                                <div class="card bg-primary text-white text-center rounded card-dashbody d-flex flex-column justify-content-center">
                                    <p>
                                        <br /><i class="fas fa-money-check-alt fa-3x" style="margin-top:9%"></i>
                                    </p>
                                    <h5 class="card-title">Once-Off Payments/ Debit Orders</h5>
                                    <p class="card-text">Make a Once-Off Payment or Add a Recurring Debit Order</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </a>
            <a href="~/DebitOrder/IndexBatchPayment">
                <div class="col-sm-12 col-md-6 col-lg-3 mb-4">
                    <div class="flip-container">
                        <div class="flip">
                            <div class="card bg-primary text-white rounded text-center flip-front">
                                <div class="card-dashbody d-flex flex-column justify-content-center">
                                    <p>
                                        <br /><i class="fas fa-money-check-alt fa-3x" style="margin-top:9%"></i>
                                    </p>
                                    <h5 class="card-title">Batch Payments</h5>
                                    <p class="card-text"></p>
                                </div>
                            </div>
                            <div class="card flip-back">
                                <div class="card bg-primary text-white text-center rounded card-dashbody d-flex flex-column justify-content-center">
                                    <p>
                                        <br /><i class="fas fa-money-check-alt fa-3x" style="margin-top:9%"></i>
                                    </p>
                                    <h5 class="card-title">Batch Payments</h5>
                                    <p class="card-text">Make a Once-Off Batch Payment Debit Order instruction for multiple municipal accounts.</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </a>
            <a href="~/Report/OnlineBills">
                <div class="col-sm-12 col-md-6 col-lg-3 mb-4">
                    <div class="flip-container">
                        <div class="flip">
                            <div style="" class="card bg-primary text-white rounded text-center flip-front">
                                <div class="card-dashbody d-flex flex-column justify-content-center">
                                    <p>
                                        <br /><i class="fas fa-file-invoice fa-3x" style="margin-top:9%"></i>
                                    </p>
                                    <h5 class="card-title">View/ Pay Municipal Bills</h5>
                                    <p class="card-text"></p>
                                </div>
                            </div>
                            <div class="card flip-back">
                                <div class="card bg-primary text-white text-center rounded card-dashbody d-flex flex-column justify-content-center">
                                    <p>
                                        <br /><i class="fas fa-file-invoice fa-3x" style="margin-top:9%"></i>
                                    </p>
                                    <h5 class="card-title">View/ Pay Municipal Bills</h5>
                                    <p class="card-text">View Your Statement and Pay Your Ekurhuleni Municipal Account (MasterPass) </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </a>
            <a href="~/Consumption/Index">
                <div class="col-sm-12 col-md-6 col-lg-3 mb-4">
                    <div class="flip-container">
                        <div class="flip">
                            <div class="card bg-primary text-white rounded text-center flip-front">
                                <div class="card-dashbody d-flex flex-column justify-content-center">
                                    <p>
                                        <br /><i class="fas fa-tachometer-alt fa-3x" style="margin-top:9%"></i>
                                    </p>
                                    <h5 class="card-title">View Consumption</h5>
                                    <p class="card-text"></p>
                                </div>
                            </div>
                            <div class="card flip-back">
                                <div class="card bg-primary text-white text-center rounded card-dashbody d-flex flex-column justify-content-center">
                                    <p>
                                        <br /><i class="fas fa-tachometer-alt fa-3x" style="margin-top:9%"></i>
                                    </p>
                                    <h5 class="card-title">View Consumption</h5>
                                    <p class="card-text">View Your Utility Consumption </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </a>
            <a href="~/Payment/PaymentHistory">
                <div class="col-sm-12 col-md-6 col-lg-3 mb-4">
                    <div class="flip-container">
                        <div class="flip">
                            <div class="card bg-primary text-white rounded text-center flip-front">
                                <div class="card-dashbody d-flex flex-column justify-content-center">
                                    <p>
                                        <br /><i class="fas fa-history fa-3x" style="margin-top:9%"></i>
                                    </p>
                                    <h5 class="card-title">Payment History</h5>
                                    <p class="card-text"></p>
                                </div>
                            </div>
                            <div class="card flip-back">
                                <div class="card bg-primary text-white text-center rounded card-dashbody d-flex flex-column justify-content-center">
                                    <p>
                                        <br /><i class="fas fa-history fa-3x" style="margin-top:9%"></i>
                                    </p>
                                    <h5 class="card-title">Payment History</h5>
                                    <p class="card-text">View Your Payment History</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </a>
            <a href="@Url.Action("Manage", "Account")">
                <div class="col-sm-12 col-md-6 col-lg-3 mb-4">
                    <div class="flip-container">
                        <div class="flip">
                            <div class="card bg-primary text-white rounded text-center flip-front">
                                <div class="card-dashbody d-flex flex-column justify-content-center">
                                    <p>
                                        <br /><i class="fas fa-user-friends fa-3x" style="margin-top:9%"></i>
                                    </p>
                                    <h5 class="card-title">Manage Login Account</h5>
                                    <p class="card-text"></p>
                                </div>
                            </div>
                            <div class="card flip-back">
                                <div class="card bg-primary text-white text-center rounded card-dashbody d-flex flex-column justify-content-center">
                                    <p>
                                        <br /><i class="fas fa-user-friends fa-3x" style="margin-top:9%"></i>
                                    </p>
                                    <h5 class="card-title">Manage Login Account</h5>
                                    <p class="card-text">Manage Your Login Account Details</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </a>
            <a href="~/Support/Index">
                <div class="col-sm-12 col-md-6 col-lg-3 mb-4" style="margin-left: 40.15%;">
                    <div class="flip-container">
                        <div class="flip">
                            <div class="card bg-primary text-white rounded text-center flip-front">
                                <div class="card-dashbody d-flex flex-column justify-content-center">
                                    <p>
                                        <br /><i class="fas fa-question fa-3x" style="margin-top:9%"></i>
                                    </p>
                                    <h5 class="card-title">Create Support Query</h5>
                                    <p class="card-text"></p>
                                </div>
                            </div>
                            <i class="fas fa-ticket-alt"></i>
                            <div class="card flip-back">
                                <div class="card bg-primary text-white text-center rounded card-dashbody d-flex flex-column justify-content-center">
                                    <p>
                                        <br /><i class="fas fa-question fa-3x" style="margin-top:9%"></i>
                                    </p>
                                    <h5 class="card-title">Create Support Query</h5>
                                    <p class="card-text">Support Queries - View Existing / Log New</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </a>

        </div>
    </div>
</body>
@if (ViewBag.Status != null)
{
    <div>
        <div class="alert alert-success">
            <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
            <strong>Success!</strong> @ViewBag.Status.
        </div>
    </div>
}
