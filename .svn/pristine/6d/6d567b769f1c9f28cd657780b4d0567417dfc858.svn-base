    <link href="~/Content/wayleaveStyles/Content/toastr.css" rel="stylesheet" />
    <script type="text/javascript" language="javascript">
        $(document).ready(function () {
            if (localStorage.expandedMenu == 0) {
                $("body").addClass('sidebar-collapse');
            }

            $('body').bind('expanded.pushMenu', function () {
                localStorage.expandedMenu = 1;
            });

            $('body').bind('collapsed.pushMenu', function () {
                localStorage.expandedMenu = 0;
            });
            var scroll_top_duration = 10;
            $('body,html').animate({
                scrollTop: 0,
            }, scroll_top_duration
            );
        });
    </script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
    <link href="~/Content/wayleaveStyles/Content/toastr.css" rel="stylesheet" />
    <style>
        body {
            background: url('../../Content/wayleaveStyles/assets/Images/Home_BG.png') no-repeat center center fixed;
            -webkit-background-size: cover;
            -moz-background-size: cover;
            -o-background-size: cover;
            background-size: cover;
            /*background-color: #d5d5d5 !important;*/
        }

        bradius {
            border-top-right-radius: 15px !important;
            border-bottom-right-radius: 15px !important;
        }

        .panel-default {
            opacity: 0.9;
            margin-top: 30px;
            border-radius: 15px !important;
            border: 0px !important;
            background-color: #fff !important
        }

        .form-group.last {
            margin-bottom: 0px;
        }

        .form-control {
            border-top-right-radius: 10px !important;
            border-bottom-right-radius: 10px !important;
            /*box-shadow: none;*/
            border: 1px solid #ccc !important;
            height: 30px !important;
            background-color: #fff !important;
        }

        .input-group .input-group-addon {
            /* border-radius: 0; */
            border-top-left-radius: 10px;
            border-bottom-left-radius: 10px;
            border-color: none !important;
            height: 30px;
            border: 0px !important
            /*background-color: #b59b5a;*/
        }

        .btn-custom {
            height: 30px !important;
            width: 150px !important;
            padding-top: 5px;
            border-radius: 10px !important
        }

        .bg-green-custom {
            background-color: #006940 !important
        }

        .has-error {
            border: 1px solid red !important;
        }

        .Anch_Back {
            text-decoration: none;
            color: #004785 !important;
        }

            .Anch_Back:hover {
                text-decoration: none;
                color: #004785 !important;
            }
    </style>
    <script>
        $(document).ready(function () {
            localStorage.clear();
            localStorage.setItem("apiBaseUrl", "@ConfigurationManager.AppSettings["Api_Url"].ToString()");
            localStorage.setItem("BaseUrl", "@ConfigurationManager.AppSettings["Base_Url"].ToString()");

        });
    </script>
